<odoo noupdate="1">

    <record model="delivery.carrier.category" id="delivery_carrier_category_delivery">
        <field name="name">Delivery</field>
        <field name="code">DEL</field>
        <field name="sequence" eval="1"/>
    </record>

    <record model="delivery.carrier.category" id="delivery_carrier_category_pickup">
        <field name="name">Pickup</field>
        <field name="code">PICK</field>
        <field name="sequence" eval="2"/>
    </record>

    <record model="delivery.carrier.category" id="delivery_carrier_category_dropoff">
        <field name="name">Drop-off</field>
        <field name="code">DROP</field>
        <field name="sequence" eval="3"/>
    </record>

</odoo>
