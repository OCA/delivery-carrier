<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="1">
	
		<!-- Automatic import Delnext shippings -->
		<record id="scheduler_import_shippings_from_delnext_action" model="ir.cron">
            <field name="name">Importer Delnext shippings</field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">3</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doal">1</field>
            <field name="model" eval="'delnext.backend'" />
            <field name="function" eval="'_scheduler_import_shippings'" />
            <field name="args" eval="" />
        </record>
        
	</data>	
</odoo>