<odoo>

    <template id="report_shipping2" inherit_id="delivery.report_shipping2">
        <xpath expr="//span[@t-field='o.carrier_id']" position="after">
            <t t-foreach="o.partner_id.delivery_schedule_ids" t-as="horary">
                <br/>
                <span class="badge dropdown o_tag_color_0" t-field="horary.display_name"/>
            </t>
        </xpath>
    </template>

</odoo>
