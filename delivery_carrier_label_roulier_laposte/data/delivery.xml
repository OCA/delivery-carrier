            <field name="deposit_slip" eval="True"/>
            <field name="partner_id" ref="delivery_carrier_label_roulier_laposte.partner_la_poste"/>
            <field name="product_id" ref="delivery_carrier_label_roulier_laposte.product_product_la_poste"/>
