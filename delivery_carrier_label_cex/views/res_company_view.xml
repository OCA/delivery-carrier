<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="cex_company_form" model="ir.ui.view">
      <field name="name">base.company.cex.form</field>
      <field name="model">res.company</field>
      <field name="inherit_id" ref="base.view_company_form" />
      <field name="arch" type="xml">
        <notebook position="inside">
           <page name="cex" string="CEX">
           <separator name="cex_separator" string="Correos Express Configuration" />
           <group name="cex_group">
               <field name="cex_test"/>
               <field name="cex_username"/>
               <field name="cex_password"/>
               <field name="cex_codRte"/>
               <field name="cex_solicitante"/>
           </group>
         </page>
       </notebook>
     </field>
   </record>
</odoo>
