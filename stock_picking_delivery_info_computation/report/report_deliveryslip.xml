<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template
        id="report_delivery_document_info"
        inherit_id="delivery.report_delivery_document2"
    >
        <xpath expr="//div[@t-if='o.shipping_weight']" position="after">
            <div t-if="o.volume" class="col-auto">
                <strong>Volume:</strong>
                <br />
                <span t-field="o.volume" />
            </div>
        </xpath>
    </template>
</odoo>
